    function DeathsEdge__DeathsEdge_End takes nothing returns nothing
        local unit DeathsEdge_Target=LoadUnitHandle(DeathsEdge__DeathsEdgeHashtable, 0, GetHandleId(GetExpiredTimer()))
        call UnitRemoveAbility(DeathsEdge_Target, 'A0BL')
        call PauseTimer(GetExpiredTimer())
        call DestroyTimer(GetExpiredTimer())
    endfunction
    function DeathsEdge__DeathsEdge takes nothing returns nothing
        local unit DeathsEdge_Target=GetSpellTargetUnit()
        local unit DeathsEdge_Caster=GetTriggerUnit()
        local integer Caster_Id=GetPlayerId(GetOwningPlayer(DeathsEdge_Caster))
        call UnitAddAbility(DeathsEdge_Target, 'A0BL') //Если прошлый дез эйдж прошёл, либо если цель та же, что была и прошлой - обновляем заново таймер
        if ( TimerGetRemaining(s__DeathsEdge__DeathsEdgeTimer[GetPlayerId(GetOwningPlayer(DeathsEdge_Caster))]) <= 0.1 or LoadUnitHandle(DeathsEdge__DeathsEdgeHashtable, 0, GetHandleId(s__DeathsEdge__DeathsEdgeTimer[Caster_Id])) == DeathsEdge_Target ) then
            call PauseTimer(s__DeathsEdge__DeathsEdgeTimer[Caster_Id])
            call DestroyTimer(s__DeathsEdge__DeathsEdgeTimer[Caster_Id])
            set s__DeathsEdge__DeathsEdgeTimer[Caster_Id]= CreateTimer()
            call TimerStart(s__DeathsEdge__DeathsEdgeTimer[Caster_Id], 10, false, function DeathsEdge__DeathsEdge_End)
            call SaveUnitHandle(DeathsEdge__DeathsEdgeHashtable, 0, GetHandleId(s__DeathsEdge__DeathsEdgeTimer[Caster_Id]), DeathsEdge_Target) //Если мы кинули дез эйдж второй раз быстрее, чем прошло его время действия - запускаем другой таймер
        else
            call PauseTimer(s__DeathsEdge__DeathsEdgeTimer[Caster_Id * 2])
            call DestroyTimer(s__DeathsEdge__DeathsEdgeTimer[Caster_Id * 2])
            set s__DeathsEdge__DeathsEdgeTimer[Caster_Id * 2]= CreateTimer()
            call TimerStart(s__DeathsEdge__DeathsEdgeTimer[Caster_Id * 2], 10, false, function DeathsEdge__DeathsEdge_End)
            call SaveUnitHandle(DeathsEdge__DeathsEdgeHashtable, 0, GetHandleId(s__DeathsEdge__DeathsEdgeTimer[Caster_Id * 2]), DeathsEdge_Target)
        endif
        call PolledWait(DistanceBetweenPoints(GetUnitLoc(DeathsEdge_Caster), GetUnitLoc(DeathsEdge_Target)) / 900 * 0.15)
        call UnitDamageTarget(DeathsEdge_Caster, DeathsEdge_Target, 150, true, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
        set DeathsEdge_Target=null
        set DeathsEdge_Caster=null
    endfunction
    function DeathsEdge__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        call TriggerRegisterAnyUnitEventBJ(t, EVENT_PLAYER_UNIT_SPELL_EFFECT)
        call TriggerAddCondition(t, Condition(function DeathsEdge__IsAbillityADeathsEdge))
        call TriggerAddAction(t, function DeathsEdge__DeathsEdge)
    endfunction

