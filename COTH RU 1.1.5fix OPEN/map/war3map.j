    function Sandbox__Refresh takes nothing returns nothing
        local group AllUnits=CreateGroup()
        local unit Unit
        call GroupEnumUnitsInRect(AllUnits, GetWorldBounds(), null)
        loop
        exitwhen ( FirstOfGroup(AllUnits) == null )
            set Unit=FirstOfGroup(AllUnits)
            call UnitResetCooldown(Unit)
            call SetUnitState(Unit, UNIT_STATE_LIFE, GetUnitState(Unit, UNIT_STATE_MAX_LIFE))
            call SetUnitState(Unit, UNIT_STATE_MANA, GetUnitState(Unit, UNIT_STATE_MAX_MANA))
            call GroupRemoveUnit(AllUnits, Unit)
        endloop
        call DestroyGroup(AllUnits)
        set AllUnits=null
        set Unit=null
    endfunction
    function Sandbox__ChangePlayerState takes nothing returns nothing
        if ( SubString(GetEventPlayerChatString(), 0, StringLength("-itempoints")) == "-itempoints" ) then
            call SetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD, S2I(SubString(GetEventPlayerChatString(), StringLength("-itempoints") + 1, StringLength(GetEventPlayerChatString()))))
        elseif ( SubString(GetEventPlayerChatString(), 0, StringLength("-gold")) == "-gold" ) then
            call SetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_LUMBER, S2I(SubString(GetEventPlayerChatString(), StringLength("-gold") + 1, StringLength(GetEventPlayerChatString()))))
        endif
    endfunction
    function Sandbox__onInit takes nothing returns nothing
        local integer index
        local trigger t
        set index=0
        loop
        exitwhen ( index > 11 )
            if ( GetPlayerSlotState(Player(index)) == PLAYER_SLOT_STATE_EMPTY or GetPlayerSlotState(Player(index)) == PLAYER_SLOT_STATE_LEFT or GetPlayerController(Player(index)) == MAP_CONTROL_COMPUTER ) then
                set Sandbox__CountofLeavedPlayers=Sandbox__CountofLeavedPlayers + 1
            endif
        set index=index + 1
        endloop
        if ( Sandbox__CountofLeavedPlayers == 11 ) then
            call ExecuteFunc("InitSandbox")
        else
            return
        endif
        set t=CreateTrigger()
        call TriggerRegisterAnyPlayerChatEvent(t , "-refresh" , false)
        call TriggerAddAction(t, function Sandbox__Refresh)
        set t=CreateTrigger()
        call TriggerRegisterAnyPlayerChatEvent(t , "-addhero" , false)
        call TriggerAddAction(t, function Sandbox__CreateWindowSelectHero)
        set t=CreateTrigger()
        call TriggerRegisterAnyPlayerChatEvent(t , "-edithero" , false)
        call TriggerAddAction(t, function Sandbox__SelectHero)
        set t=CreateTrigger()
        call TriggerRegisterAnyPlayerChatEvent(t , "-ally" , false)
        call TriggerRegisterAnyPlayerChatEvent(t , "-enemy" , false)
        call TriggerRegisterAnyPlayerChatEvent(t , "-onlyme" , false)
        call TriggerAddAction(t, function Sandbox__ChangeAllyType)
        set t=CreateTrigger()
        call TriggerRegisterAnyPlayerChatEvent(t , "-itempoints" , false)
        call TriggerRegisterAnyPlayerChatEvent(t , "-gold" , false)
        call TriggerAddAction(t, function Sandbox__ChangePlayerState)
    endfunction

