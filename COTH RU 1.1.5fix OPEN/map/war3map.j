    function Camera__ChangeCamera takes nothing returns nothing
        local boolean ZoomMin=S2I(SubString(GetEventPlayerChatString(), 6, 10)) >= 1650
        local boolean ZoomMax=S2I(SubString(GetEventPlayerChatString(), 6, 10)) <= 4000
        local boolean ExactOnly=SubString(GetEventPlayerChatString(), 0, 6) == "-zoom " and SubString(GetEventPlayerChatString(), 10, StringLength(GetEventPlayerChatString())) == ""
        if ( GetEventPlayerChatString() == "-far" ) then
            call SetCameraFieldForPlayer(GetTriggerPlayer(), CAMERA_FIELD_TARGET_DISTANCE, 2975, 0.5)
            return
        endif
        if ( GetEventPlayerChatString() == "-medium" ) then
            call SetCameraFieldForPlayer(GetTriggerPlayer(), CAMERA_FIELD_TARGET_DISTANCE, 2300, 0.5)
            return
        endif
        if ( GetEventPlayerChatString() == "-close" ) then
            call ResetToGameCameraForPlayer(GetTriggerPlayer(), 0.5)
            return
        endif
        if ( ZoomMin and ZoomMax and ExactOnly ) then
            call SetCameraFieldForPlayer(GetTriggerPlayer(), CAMERA_FIELD_TARGET_DISTANCE, S2R(SubString(GetEventPlayerChatString(), 6, 10)), 0.5)
        endif
    endfunction
    function Camera__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        local integer index
        set index=0
        loop
        exitwhen ( index > 11 )
            call TriggerRegisterPlayerChatEvent(t, Player(index), "-far", true)
            call TriggerRegisterPlayerChatEvent(t, Player(index), "-medium", true)
            call TriggerRegisterPlayerChatEvent(t, Player(index), "-close", true)
            call TriggerRegisterPlayerChatEvent(t, Player(index), "-zoom", false)
        set index=index + 1
        endloop
        call TriggerAddAction(t, function Camera__ChangeCamera)
    endfunction

