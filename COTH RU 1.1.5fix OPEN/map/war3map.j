        endif
        set HeroArmorStr=R2SW(HeroArmor, 2, 2)
        if ( HeroArmor >= 100 ) then
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', S2I(SubString(HeroArmorStr, 0, 1)) + 1)
