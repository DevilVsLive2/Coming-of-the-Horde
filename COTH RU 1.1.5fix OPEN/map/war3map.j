        if ( UnitHasBuffBJ(whichUnit, 'Binf') ) then
            set DamageFromAbils=DamageFromAbils + BasicDamage * 1.1
        endif
        if ( UnitHasBuffBJ(whichUnit, 'BNso') ) then
            set DamageFromAbils=DamageFromAbils + BasicDamage * ( 1 + ( GetUnitAbilityLevel(DaelinUnit, 'ACfd') * 4 + 2 ) / 100 )
        endif
        if ( R2I(DamageFromAbils) > BasicDamage ) then
            set DamageFromAbils=DamageFromAbils - BasicDamage
        endif
        return R2I(DamageFromAbils)
    endfunction
