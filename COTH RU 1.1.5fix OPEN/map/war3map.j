        function CurdranAbils__anon__0 takes nothing returns boolean
            return ( GetUnitTypeId(GetTriggerUnit()) == Curdran ) or ( GetUnitTypeId(GetTriggerUnit()) == CurdranMorphFirst ) or ( GetUnitTypeId(GetTriggerUnit()) == CurdranMorphSecond ) or ( GetUnitTypeId(GetTriggerUnit()) == CurdranMorphThird )
        endfunction
            function CurdranAbils__anon__2 takes nothing returns nothing
                local real targetX=LoadReal(abilityData, GetHandleId(GetExpiredTimer()), 0)
                local real targetY=LoadReal(abilityData, GetHandleId(GetExpiredTimer()), 1)
                local unit stormHammerMissle=LoadUnitHandle(abilityData, GetHandleId(GetExpiredTimer()), 0)
                local real distanceBetweenMissleAndTargetPoint=SquareRoot(Pow(targetX - GetUnitX(stormHammerMissle), 2) + Pow(targetY - GetUnitY(stormHammerMissle), 2))
                local real angleToTargetPoint=Atan2(targetY - GetUnitY(stormHammerMissle), targetX - GetUnitX(stormHammerMissle))
                local unit caster=LoadUnitHandle(abilityData, GetHandleId(GetExpiredTimer()), 1)
                local real newMissleX=GetUnitX(stormHammerMissle) + CurdranAbils__stormHammerSpeedPerUpdate * Cos(angleToTargetPoint)
                local real newMissleY=GetUnitY(stormHammerMissle) + CurdranAbils__stormHammerSpeedPerUpdate * Sin(angleToTargetPoint)
                local real newDistanceBetweenMissleAndTargetPoint=SquareRoot(Pow(targetX - newMissleX, 2) + Pow(targetY - newMissleY, 2))
                local effect boomEffect=null
                local effect lightningEffect=null
                local group targets=null
                local unit target=null
                local unit slowDummyCaster=null
                if ( newDistanceBetweenMissleAndTargetPoint > distanceBetweenMissleAndTargetPoint ) then //Missle become further from target point, move to target point, and make boom
                    call SetUnitX(stormHammerMissle, targetX)
                    call SetUnitY(stormHammerMissle, targetY)
                    call UnitApplyTimedLife(stormHammerMissle, 'BTLF', .01)
                    set boomEffect=AddSpecialEffect("Abilities\\Spells\\Human\\Thunderclap\\ThunderClapCaster.mdl", targetX, targetY)
                    call TerrainDeformCrater(targetX, targetY, CurdranAbils__stormHammerAreaOfEffect, 32, 250, false)
                    set targets=CreateGroup()
                    call GroupEnumUnitsInRange(targets, targetX, targetY, CurdranAbils__stormHammerAreaOfEffect + 150, null)
                    loop
                    exitwhen ( FirstOfGroup(targets) == null )
                        set target=FirstOfGroup(targets)
                        call GroupRemoveUnit(targets, target)
                        if ( IsUnitInRangeXY(target, targetX, targetY, CurdranAbils__stormHammerAreaOfEffect) and IsUnitEnemy(target, GetOwningPlayer(caster)) and IsUnitType(target, UNIT_TYPE_GROUND) and not ( IsUnitMagicImmune(target) ) ) then
