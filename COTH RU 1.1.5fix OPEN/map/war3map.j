        local real ArmorCount=0
        local integer HeroAgi=GetHeroAgiOnlyBonus(whichHero)
        local integer HeroDefaultAgi=GetHeroAgi(whichHero, false)
        local real AGI_DEFENSE_BONUS=0.2
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Breast_Plate_of_the_Lightbringer_Inventory) * 16
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Ring_of_Protection) * 2
