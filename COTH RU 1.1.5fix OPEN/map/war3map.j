                    call DestroyTimer(GetExpiredTimer())
                    set stormHammerMissle=null
                    return
                endif
                call SetUnitX(stormHammerMissle, newMissleX)
                call SetUnitY(stormHammerMissle, newMissleY)
                set stormHammerMissle=null
            endfunction
