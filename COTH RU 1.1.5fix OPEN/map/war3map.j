    function RejuventationSpell__clearRejuventationDataForCaster takes unit caster returns nothing
        local integer casterId=GetHandleId(caster)
        local effect yellowBall=LoadEffectHandle(RejuventationSpell__rejuventationData, casterId, 0)
        local effect greenBall=LoadEffectHandle(RejuventationSpell__rejuventationData, casterId, 1)
        local timer healTimer=LoadTimerHandle(RejuventationSpell__rejuventationData, casterId, 2)
        local trigger onCasterDamaged=LoadTriggerHandle(RejuventationSpell__rejuventationData, casterId, 3)
        call DestroyEffect(yellowBall)
        call DestroyEffect(greenBall)
        call FlushChildHashtable(RejuventationSpell__rejuventationData, GetHandleId(healTimer))
        call DestroyTimer(healTimer)
        call TriggerClearConditions(onCasterDamaged)
        call DestroyTrigger(onCasterDamaged)
        call FlushChildHashtable(RejuventationSpell__rejuventationData, casterId)
    endfunction
        function RejuventationSpell__anon__0 takes nothing returns boolean
            return GetSpellAbilityId() == 'AEsf'
        endfunction
            function RejuventationSpell__anon__2 takes nothing returns boolean
                call IssueImmediateOrder(GetTriggerUnit(), "stop")
                return true
            endfunction
        function RejuventationSpell__anon__1 takes nothing returns nothing
            local unit caster=GetTriggerUnit()
            local integer casterId=GetHandleId(caster)
            local trigger onCasterDamaged=null
            local effect yellowBall=LoadEffectHandle(RejuventationSpell__rejuventationData, casterId, 0)
            call RejuventationSpell__clearRejuventationDataForCaster(caster)
            set yellowBall=AddSpecialEffectTarget("RejuvTarget.mdx", caster, "overhead")
            call SaveEffectHandle(RejuventationSpell__rejuventationData, casterId, 0, yellowBall)
            set onCasterDamaged=CreateTrigger()
            call TriggerRegisterUnitEvent(onCasterDamaged, caster, EVENT_UNIT_DAMAGED)
            call TriggerAddCondition(onCasterDamaged, Filter(function RejuventationSpell__anon__2))
            set caster=null
            set yellowBall=null
            set onCasterDamaged=null
        endfunction
        function RejuventationSpell__anon__3 takes nothing returns boolean
            return GetSpellAbilityId() == 'AEsf'
        endfunction
            function RejuventationSpell__anon__5 takes nothing returns boolean
                call RejuventationSpell__clearRejuventationDataForCaster(GetTriggerUnit())
                return true
            endfunction
            function RejuventationSpell__anon__6 takes nothing returns nothing
                local unit caster=LoadUnitHandle(RejuventationSpell__rejuventationData, GetHandleId(GetExpiredTimer()), 0)
                local integer healNumber=LoadInteger(RejuventationSpell__rejuventationData, GetHandleId(GetExpiredTimer()), 0)
                set healNumber=healNumber + 1
                call SaveInteger(RejuventationSpell__rejuventationData, GetHandleId(GetExpiredTimer()), 0, healNumber)
                call AddUnitLifePercent(caster , 4 + healNumber)
                if ( healNumber >= 12 ) then
                    call RejuventationSpell__clearRejuventationDataForCaster(caster)
                endif
                set caster=null
            endfunction
        function RejuventationSpell__anon__4 takes nothing returns nothing
            local unit caster=GetTriggerUnit()
            local integer casterId=GetHandleId(caster)
            local effect greenBall=LoadEffectHandle(RejuventationSpell__rejuventationData, casterId, 1)
            local timer healTimer=LoadTimerHandle(RejuventationSpell__rejuventationData, casterId, 2)
            local trigger onCasterDamaged=LoadTriggerHandle(RejuventationSpell__rejuventationData, casterId, 3)
            call RejuventationSpell__clearRejuventationDataForCaster(caster)
            set greenBall=AddSpecialEffectTarget("Abilities\\Spells\\NightElf\\Tranquility\\TranquilityTarget.mdl", caster, "overhead")
            call SaveEffectHandle(RejuventationSpell__rejuventationData, casterId, 1, greenBall)
            set onCasterDamaged=CreateTrigger()
            call TriggerRegisterUnitEvent(onCasterDamaged, caster, EVENT_UNIT_DAMAGED)
            call TriggerAddCondition(onCasterDamaged, Filter(function RejuventationSpell__anon__5))
            set healTimer=CreateTimer()
            call SaveUnitHandle(RejuventationSpell__rejuventationData, GetHandleId(healTimer), 0, caster)
            call SaveInteger(RejuventationSpell__rejuventationData, GetHandleId(healTimer), 0, 1)
            call TimerStart(healTimer, 2, true, function RejuventationSpell__anon__6)
            set onCasterDamaged=null
            set healTimer=null
            set greenBall=null
            set caster=null
        endfunction
        function RejuventationSpell__anon__7 takes nothing returns boolean
            return GetSpellAbilityId() == 'AEsf'
        endfunction
        function RejuventationSpell__anon__8 takes nothing returns nothing
            call RejuventationSpell__clearRejuventationDataForCaster(GetTriggerUnit())
        endfunction
    function RejuventationSpell__onInit takes nothing returns nothing
        local trigger onRejuvSpellEffect=CreateTrigger()
