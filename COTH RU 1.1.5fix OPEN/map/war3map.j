    function TestItemAq__CountOfItemPointsForItem takes integer ItemID,unit whichUnit returns integer
        local integer i
        local integer SubItem
        local integer price=LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 6)
        if ( TestItemAq__UnitHasItemOfType(whichUnit , ItemID) ) then //BJDebugMsg("У юнита есть айтем " + GetObjectName(ItemID) + " возвращаю 0");
            set s__TestItemAq__RemovedItem[TestItemAq__RemovedItemIndex]= ItemID
            set s__TestItemAq__RemovedItemPosition[TestItemAq__RemovedItemIndex]= TestItemAq__ItemIndex(ItemID , whichUnit)
            set TestItemAq__RemovedItemIndex=TestItemAq__RemovedItemIndex + 1
            call RemoveItem(UnitItemInSlot(whichUnit, TestItemAq__ItemIndex(ItemID , whichUnit)))
            return 0
        endif
        set i=0 //Проверяем все 6 слотов для под-айтемов, нашего айтема-цели
        loop
        exitwhen ( i > 5 )
            set SubItem=LoadInteger(TestItemAq__ItemDataHashtable, ItemID, i) //Если у нас есть этот предмет - удаляем его и сохраняем айди
            if ( TestItemAq__UnitHasItemOfType(whichUnit , SubItem) and SubItem > 0 ) then
                set s__TestItemAq__RemovedItem[TestItemAq__RemovedItemIndex]= SubItem
                set s__TestItemAq__RemovedItemPosition[TestItemAq__RemovedItemIndex]= TestItemAq__ItemIndex(SubItem , whichUnit)
                set TestItemAq__RemovedItemIndex=TestItemAq__RemovedItemIndex + 1
                call RemoveItem(UnitItemInSlot(whichUnit, TestItemAq__ItemIndex(SubItem , whichUnit)))
            elseif ( LoadInteger(TestItemAq__ItemDataHashtable, SubItem, 0) != 0 ) then //Если у нас нет этого предмета, но он из чего-то собирается - вызываем эту же функцию, для проверки уже его цены
                set price=price + TestItemAq__CountOfItemPointsForItem(SubItem , whichUnit)
            elseif ( LoadInteger(TestItemAq__ItemDataHashtable, SubItem, 0) == 0 and not ( TestItemAq__UnitHasItemOfType(whichUnit , SubItem) ) ) then //Если у нас нет этого прдемета и он не собирается - добавляем его цену
                set price=price + LoadInteger(TestItemAq__ItemDataHashtable, SubItem, 6)
            endif
        set i=i + 1
        endloop
        return price
    endfunction
