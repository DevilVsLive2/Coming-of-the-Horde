    endfunction
    function TestItemAq__GetHeroFreeSlots takes unit whichHero returns integer
        local integer index
        local integer count=0
        set index=0
        loop
        exitwhen ( index > 5 )
            if ( GetItemTypeId(UnitItemInSlot(whichHero, index)) == 0 ) then
                set count=count + 1
