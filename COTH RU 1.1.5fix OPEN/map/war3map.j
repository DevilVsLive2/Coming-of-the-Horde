        set TeronAura__TrigAura=null
        set TeronAura__TrigAura=CreateTrigger()
        call TriggerRegisterTimerEvent(TeronAura__TrigAura, 0.1, true)
        set TeronAura__AuraAction=TriggerAddAction(TeronAura__TrigAura, function TeronAura__TeronEnumUnits)
    endfunction

