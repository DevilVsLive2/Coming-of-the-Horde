        call TriggerRegisterAnyUnitEventBJ(onRejuvSpellEffect, EVENT_PLAYER_UNIT_SPELL_EFFECT)
        call TriggerAddCondition(onRejuvSpellEffect, Filter(function RejuventationSpell__anon__0))
