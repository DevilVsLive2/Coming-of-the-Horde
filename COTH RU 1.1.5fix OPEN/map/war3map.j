        if ( UnitHasItemOfTypeBJ(Damager, Amani_Throw_Axe_Inventory) and not IsTargetAllyDamager ) then
            if ( IsHeroUnitId(GetUnitTypeId(Target)) ) then //Добавление абилки на -7 армора
                call UnitAddAbility(Target, AMANI_DEBUFF)
                set TempTimer=LoadTimerHandle(TimerHashtable, GetHandleId(Target), 0) //Смотрим, имеется ли у нас уже дебафф на юните
                if ( TempTimer != null ) then //Если имеется - мы перезапускаем таймер, что убирает дебаф
                    call PauseTimer(TempTimer)
                    call DestroyTimer(TempTimer)
                    set TempTimer=CreateTimer()
                    call TimerStart(TempTimer, AmaniTimerSeconds, false, function UnitDamaged__AmaniRemove)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(TempTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_AMAIN_DEBUFF, Target)
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 0, TempTimer)
                else
                    set AmaniThrowAxeTimer=CreateTimer()
                    call TimerStart(AmaniThrowAxeTimer, AmaniTimerSeconds, false, function UnitDamaged__AmaniRemove)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(AmaniThrowAxeTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_AMAIN_DEBUFF, Target)
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 0, AmaniThrowAxeTimer)
                endif
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Blackrock_Champions_Axe_Inventory) and not IsTargetAllyDamager ) then //Топор блэкрока
            if ( IsHeroUnitId(GetUnitTypeId(Target)) ) then //Добавление абилки на -7 армора
                call UnitAddAbility(Target, BLACKROCK_DEBUFF)
                set TempTimer=LoadTimerHandle(TimerHashtable, GetHandleId(Target), 1) //Смотрим, имеется ли у нас уже дебафф на юните
                if ( TempTimer != null ) then //Если имеется - мы перезапускаем таймер, что убирает дебаф
                    call PauseTimer(TempTimer)
                    call DestroyTimer(TempTimer)
                    set TempTimer=CreateTimer()
                    call TimerStart(TempTimer, BlackrockTimerSeconds, false, function UnitDamaged__BlackrockRemove) //Сохраняем в хендл таймера юнита, что бы когда он кончается знать, на ком дебафф
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(TempTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_BLACKROCKS_AXE_DEBUFF, Target) //Сохраняем в хендл юнита таймер, что бы мы могли в этом триггере проверить, идёт ли таймер для этого
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 1, TempTimer)
                else
                    set BlackrockChampionsAxeTimer=CreateTimer()
                    call TimerStart(BlackrockChampionsAxeTimer, BlackrockTimerSeconds, false, function UnitDamaged__BlackrockRemove)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(BlackrockChampionsAxeTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_BLACKROCKS_AXE_DEBUFF, Target)
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 1, BlackrockChampionsAxeTimer)
                endif
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Corruption_Edge_Inventory) and not IsTargetAllyDamager ) then //Меч Рока
            if ( IsHeroUnitId(GetUnitTypeId(Target)) ) then
                call RemoveUnit(s__UnitDamaged__CorruptionAuraUnit[GetPlayerId(GetOwningPlayer(Target))])
                set Placeholder=CreateUnit(GetOwningPlayer(Target), 'o008', 0, 0, 0)
                set s__UnitDamaged__CorruptionAuraUnit[GetPlayerId(GetOwningPlayer(Target))]= Placeholder //Добавление абилок, снижающих защиту
                call UnitAddAbility(Target, 'A0GO')
                call UnitAddAbility(Target, 'A0GW')
                call UnitAddAbility(Target, 'A0GX')
                set TempTimer=LoadTimerHandle(TimerHashtable, GetHandleId(Target), 2) //Смотрим, имеется ли у нас уже дебафф на юните
                if ( TempTimer != null ) then //Если имеется - мы перезапускаем таймер, что убирает дебаф
                    call PauseTimer(TempTimer)
                    call DestroyTimer(TempTimer)
                    set TempTimer=CreateTimer()
                    call TimerStart(TempTimer, CorruptionEdgeTimerSeconds, false, function UnitDamaged__CorruptionEdgeRemove) //Сохраняем в хендл таймера юнита, что бы когда он кончается знать, на ком дебафф
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(TempTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_CORRUPTION_EDGE_DEBUFF, Target)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(TempTimer), UnitDamaged__HASHTABLE_CORRUPTION_AURA, Placeholder) //Сохраняем в хендл юнита таймер, что бы мы могли в этом триггере проверить, идёт ли таймер для этого
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 2, TempTimer)
                else
                    set CorruptionEdgeTimer=CreateTimer()
                    call TimerStart(CorruptionEdgeTimer, CorruptionEdgeTimerSeconds, false, function UnitDamaged__CorruptionEdgeRemove)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(CorruptionEdgeTimer), UnitDamaged__HASHTABLE_UNIT_REMOVE_CORRUPTION_EDGE_DEBUFF, Target)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(CorruptionEdgeTimer), UnitDamaged__HASHTABLE_CORRUPTION_AURA, Placeholder)
                    call SaveTimerHandle(TimerHashtable, GetHandleId(Target), 2, CorruptionEdgeTimer)
                endif
                call SetCorruptionEdgeLevels(Target , Placeholder)
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Thunderfury_Inventory) and not IsTargetAllyDamager ) then
            set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
            call UnitAddAbility(Placeholder, 'A0BY')
            call IssueTargetOrder(Placeholder, "slow", Target)
            call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
            if ( GetRandomInt(1, 100) <= 40 ) then
                call UnitAddAbility(Placeholder, 'ACcl')
                call DisableTrigger(TriggeringTrg)
                call IssueTargetOrder(Placeholder, "chainlightning", Target)
                call EnableTrigger(TriggeringTrg)
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Obsidian_Spear_Inventory) and not IsTargetAllyDamager ) then
            if ( GetRandomInt(1, 100) <= 18 ) then
                if ( not ( IsUnitType(Target, UNIT_TYPE_HERO) ) and not ( IsUnitType(Target, UNIT_TYPE_ANCIENT) ) and not ( IsUnitType(Target, UNIT_TYPE_STRUCTURE) ) ) then
                    set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
                    call DisableTrigger(TriggeringTrg)
                    call UnitDamageTarget(Placeholder, Target, 50000, true, false, ATTACK_TYPE_CHAOS, DAMAGE_TYPE_UNIVERSAL, WEAPON_TYPE_WHOKNOWS)
                    call EnableTrigger(TriggeringTrg)
                    call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                endif
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Argent_Spear_Inventory) and not IsTargetAllyDamager ) then
            if ( GetRandomInt(1, 100) <= 8 ) then
                if ( not ( IsUnitType(Target, UNIT_TYPE_HERO) ) and not ( IsUnitType(Target, UNIT_TYPE_ANCIENT) ) and not ( IsUnitType(Target, UNIT_TYPE_STRUCTURE) ) ) then
                    set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
                    call DisableTrigger(TriggeringTrg)
                    call UnitDamageTarget(Placeholder, Target, 50000, true, false, ATTACK_TYPE_CHAOS, DAMAGE_TYPE_UNIVERSAL, WEAPON_TYPE_WHOKNOWS)
                    call EnableTrigger(TriggeringTrg)
                    call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                endif
            endif
        endif
        if ( GetUnitAbilityLevel(Damager, 'A03L') > 0 and not IsTargetAllyDamager ) then //Кара Бутера
            if ( GetUnitAbilityLevel(Target, 'BEsh') > 0 ) then
                if ( GetRandomInt(1, 100) <= ( GetUnitAbilityLevel(Damager, 'A03L') * 5 + 5 ) * 2 ) then
                    set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
                    call UnitAddAbility(Placeholder, 'ACbh')
                    call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                    if ( IsUnitType(Target, UNIT_TYPE_HERO) or GetUnitTypeId(Target) == 'nogr' or GetUnitTypeId(Target) == 'nsqa' or GetUnitTypeId(Target) == 'nahy' ) then
                        call SetUnitAbilityLevel(Placeholder, 'ACbh', 1)
                    else
                        call SetUnitAbilityLevel(Placeholder, 'ACbh', 2)
                    endif
                    call DisableTrigger(TriggeringTrg)
                    call IssueTargetOrder(Placeholder, "thunderbolt", Target)
                    call EnableTrigger(TriggeringTrg)
                endif
            elseif ( GetRandomInt(1, 100) <= GetUnitAbilityLevel(Damager, 'A03L') * 5 + 5 ) then
                set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
                call UnitAddAbility(Placeholder, 'ACbh')
                call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                if ( IsUnitType(Target, UNIT_TYPE_HERO) or GetUnitTypeId(Target) == 'nogr' or GetUnitTypeId(Target) == 'nsqa' or GetUnitTypeId(Target) == 'nahy' ) then
                    call SetUnitAbilityLevel(Placeholder, 'ACbh', 1)
                else
                    call SetUnitAbilityLevel(Placeholder, 'ACbh', 2)
                endif
                call DisableTrigger(TriggeringTrg)
                call IssueTargetOrder(Placeholder, "thunderbolt", Target)
                call EnableTrigger(TriggeringTrg)
            endif
        endif
        if ( GetUnitAbilityLevel(Damager, 'A09E') > 0 and not IsTargetAllyDamager ) then //Вражеская Слабость Темара
            if ( not ( IsUnitType(Target, UNIT_TYPE_STRUCTURE) ) and not ( IsUnitType(Target, UNIT_TYPE_MECHANICAL) ) and GetUnitAbilityLevel(Target, 'B01E') < 1 and GetUnitAbilityLevel(Target, 'B03N') < 1 and GetUnitState(Target, UNIT_STATE_LIFE) < ( 125 + I2R(GetUnitAbilityLevel(Damager, 'A09E') * 125) ) and GetUnitTypeId(Target) != 'ohwd' and GetUnitTypeId(Target) != 'oswy' ) then
                set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 270)
                call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                call DisableTrigger(TriggeringTrg)
                call UnitDamageTarget(Placeholder, Target, 9000, true, false, ATTACK_TYPE_CHAOS, DAMAGE_TYPE_UNIVERSAL, WEAPON_TYPE_WHOKNOWS)
                call EnableTrigger(TriggeringTrg)
            endif
        endif
        if ( GetUnitAbilityLevel(Damager, 'A0CJ') > 0 and not IsTargetAllyDamager ) then
            call DisableTrigger(TriggeringTrg)
            call UnitDamageTarget(Damager, Target, GetUnitState(Target, UNIT_STATE_MANA) * 0.02, true, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
            call EnableTrigger(TriggeringTrg)
            call UnitRemoveMana(Target , GetUnitState(Target, UNIT_STATE_MANA) * 0.02)
        endif
        if ( ( GetUnitTypeId(Damager) == 'O001' or GetUnitTypeId(Damager) == 'O000' or GetUnitTypeId(Damager) == 'O00N' ) and not IsTargetAllyDamager ) then
            call TriggerSleepAction(0.05)
            if ( UnitDamaged__IsGaronaUsedAbillity == false ) then
                if ( not ( UnitHasBuffBJ(Target, 'BHds') ) and not ( UnitHasBuffBJ(Target, 'B03N') ) and not ( UnitHasBuffBJ(Target, 'BIsv') ) ) then
                    call DisableTrigger(TriggeringTrg)
                    call UnitDamageTarget(Damager, Target, l__Damage * ( 1 + ( GetUnitAbilityLevel(Damager, 'A09D') ) ), true, false, ATTACK_TYPE_MELEE, DAMAGE_TYPE_NORMAL, WEAPON_TYPE_WHOKNOWS)
                    call UnitDamageTarget(Damager, Target, l__Damage * ( 1 + ( GetUnitAbilityLevel(Damager, 'A09D') ) ), true, false, ATTACK_TYPE_MELEE, DAMAGE_TYPE_UNIVERSAL, WEAPON_TYPE_WHOKNOWS)
                    call EnableTrigger(TriggeringTrg)
                    set GaronaDamage=CreateTextTagLocBJ(I2S(R2I(l__Damage * ( 2 + ( GetUnitAbilityLevel(Damager, 'A09D') ) ))) + "!", GetUnitLoc(Target), 0, 10, 100, 0.00, 0.00, 0)
                    call SetTextTagPermanentBJ(GaronaDamage, false)
                    call SetTextTagVelocityBJ(GaronaDamage, 15.00, 90)
                    call SetTextTagLifespanBJ(GaronaDamage, 3.00)
                    call SetTextTagFadepointBJ(GaronaDamage, 1.00)
                endif
                if ( IsUnitOwnedByPlayer(Damager, Player(9)) ) then
                    call UnitAddAbility(Damager, 'A0DH')
                    call UnitRemoveAbility(Damager, 'A0DH')
                elseif ( IsUnitOwnedByPlayer(Damager, Player(10)) ) then
                    call UnitAddAbility(Damager, 'A029')
                    call UnitRemoveAbility(Damager, 'A029')
                else
                    call UnitAddAbility(Damager, 'A0DG')
                    call UnitRemoveAbility(Damager, 'A0DG')
                endif
                call SetPlayerAbilityAvailableBJ(true, 'Afod', GetOwningPlayer(Damager))
                call SetPlayerAbilityAvailableBJ(true, 'A099', GetOwningPlayer(Damager))
                call SetPlayerAbilityAvailableBJ(true, 'AOwk', GetOwningPlayer(Damager))
                call SetPlayerAbilityAvailableBJ(false, 'ANwk', GetOwningPlayer(Damager))
            endif
        endif
        if ( Damager == GromUnit and not IsTargetAllyDamager ) then
            set udg_SliceandDiceOff=1
        endif
        if ( Damager == TichondriusUnit and GetUnitState(Damager, UNIT_STATE_MANA) >= 12 and udg_DarkTouchOn == 1 and not IsTargetAllyDamager ) then
            set udg_DarkTouchOff=1
        endif
        if ( Damager == ZulJinUnit and not IsTargetAllyDamager ) then
            set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 0)
            call UnitAddAbility(Placeholder, 'ACcr')
            call IssueTargetOrder(Placeholder, "cripple", Target)
            call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
            if ( UnitDamaged__PrevTarget == Target ) then
                set UnitDamaged__udg_CarveDamage=UnitDamaged__udg_CarveDamage + 4 * GetUnitAbilityLevel(Damager, 'AIad')
            else
                set UnitDamaged__udg_CarveDamage=4 * GetUnitAbilityLevel(Damager, 'AIad')
            endif
            call DisableTrigger(TriggeringTrg)
            call UnitDamageTarget(Damager, Target, UnitDamaged__udg_CarveDamage, true, true, ATTACK_TYPE_PIERCE, DAMAGE_TYPE_NORMAL, WEAPON_TYPE_WHOKNOWS)
            call EnableTrigger(TriggeringTrg)
        endif
        set UnitDamaged__PrevTarget=Target
        set BashCooldownTimer=null
        set Placeholder=null
        set TempTimer=null
        set AmaniThrowAxeTimer=null
        set BlackrockChampionsAxeTimer=null
        set CorruptionEdgeTimer=null
        set TriggeringTrg=null
        set TempUnit=null
        set TempLoc=null
        set TempLocSecond=null
        set GaronaDamage=null
    endfunction
    function UnitDamaged__EventUnitDeath takes nothing returns nothing
        if ( GetKillingUnit() == GaronaUnit and IsUnitEnemy(GetDyingUnit(), GetOwningPlayer(GetKillingUnit())) and IsUnitType(GetDyingUnit(), UNIT_TYPE_HERO) ) then
            call SetUnitPositionLoc(gg_unit_osw3_0435, GetUnitLoc(GetDyingUnit()))
            call SetUnitAbilityLevelSwapped('A0BA', gg_unit_osw3_0435, GetUnitAbilityLevelSwapped('A099', GaronaUnit))
            call IssueImmediateOrder(gg_unit_osw3_0435, "thunderclap")
            call PlaySoundOnUnitBJ(gg_snd_FanOfKnives, 100, GaronaUnit)
        endif
    endfunction
    function UnitDamaged__IsUnitGarona takes nothing returns boolean
        return ( GetUnitTypeId(GetTriggerUnit()) == 'O001' or GetUnitTypeId(GetTriggerUnit()) == 'O000' or GetUnitTypeId(GetTriggerUnit()) == 'O00N' )
    endfunction
    function UnitDamaged__DisableGaronaTriggeringUltToAbility takes nothing returns nothing
        set UnitDamaged__IsGaronaUsedAbillity=true
        call TriggerSleepAction(0.1)
        set UnitDamaged__IsGaronaUsedAbillity=false
    endfunction
    function UnitDamaged__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        return
        call TriggerRegisterAnyUnitEventBJ(t, EVENT_PLAYER_UNIT_ATTACKED)
        call TriggerAddAction(t, function UnitDamaged__UnitOnDamage)
        set t=CreateTrigger()
        call TriggerRegisterPlayerUnitEvent(t, Player(9), EVENT_PLAYER_UNIT_SPELL_EFFECT, Condition(function UnitDamaged__IsUnitGarona))
        call TriggerAddAction(t, function UnitDamaged__DisableGaronaTriggeringUltToAbility)
    endfunction

