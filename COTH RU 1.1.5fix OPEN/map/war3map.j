        local integer BasicDamage=LoadInteger(UnitInfoHashtable, GetUnitTypeId(whichUnit), HERO_BASIC_DAMAGE) + GetHeroStatBJ(LoadInteger(UnitInfoHashtable, GetUnitTypeId(whichUnit), HERO_MAIN_ATTR), whichUnit, false)
