            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', S2I(SubString(HeroArmorStr, 2, 3)) + 1)
        elseif ( HeroArmor >= 10 ) then
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GN', S2I(SubString(HeroArmorStr, 0, 1)) + 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', S2I(SubString(HeroArmorStr, 1, 2)) + 1)
        elseif ( HeroArmor >= 1 ) then
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GN', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', S2I(SubString(HeroArmorStr, 0, 1)) + 1)
        elseif ( HeroArmor >= 0.1 ) then
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GN', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', 1)
        elseif ( HeroArmor >= 0.01 ) then
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GN', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', 1)
        else
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GP', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GN', 1)
            call SetUnitAbilityLevel(CorruptionEdgeAura, 'A0GO', 1)
