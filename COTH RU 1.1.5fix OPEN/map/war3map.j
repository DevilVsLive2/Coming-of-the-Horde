    function InitSandbox takes nothing returns nothing
        local quest CommandInfo=CreateQuest()
        local questitem CommandInfoItem=QuestCreateItem(CommandInfo)
        local integer PlayerIndex
        call SetForceAllianceStateBJ(GetPlayersAll(), GetPlayersAll(), bj_ALLIANCE_ALLIED_ADVUNITS)
        call SetForceAllianceStateBJ(GetPlayersAll(), bj_FORCE_PLAYER[PLAYER_NEUTRAL_AGGRESSIVE], bj_ALLIANCE_UNALLIED_VISION)
        call SetForceAllianceStateBJ(bj_FORCE_PLAYER[PLAYER_NEUTRAL_AGGRESSIVE], GetPlayersAll(), bj_ALLIANCE_UNALLIED_VISION)
        set PlayerIndex=0
        loop
        exitwhen ( PlayerIndex > 11 )
            call SetPlayerState(Player(PlayerIndex), PLAYER_STATE_RESOURCE_GOLD, 500000)
            call SetPlayerState(Player(PlayerIndex), PLAYER_STATE_RESOURCE_LUMBER, 500000)
        set PlayerIndex=PlayerIndex + 1
        endloop
        call ClearTextMessages()
        call DisplayTimedTextToPlayer(GetLocalPlayer(), 0, 0, 30, "Добро пожаловать в режим Песочницы. Ознакомиться со всеми командами вы можете в F9")
        call QuestSetTitle(CommandInfo, "Команды")
        call QuestItemSetDescription(CommandInfoItem, "Команды Песочницы")
        set Sandbox__tempString[0]="Что бы сбросить перезарядку способностей напишите -refresh.|n"
        set Sandbox__tempString[1]="Что бы добавить героя напишите -addhero|n"
