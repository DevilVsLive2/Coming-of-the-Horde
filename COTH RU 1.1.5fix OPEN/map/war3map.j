    function TestItemAq__onItemBuy takes nothing returns nothing
        local integer ItemID=GetItemTypeId(GetManipulatedItem())
        local integer i
        local integer price=LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 6)
        local integer TargetItemID
        local effect e=null
        if ( GetItemTypeId(GetManipulatedItem()) == 'stwp' or GetItemTypeId(GetManipulatedItem()) == 'plcl' or GetItemTypeId(GetManipulatedItem()) == 'hslv' ) then
            call DisplayTimedTextToPlayer(GetOwningPlayer(GetTriggerUnit()), 0, 0, 15, "Этот предмет более не доступен!")
            call RemoveItem(GetItemOfTypeFromUnitBJ(GetTriggerUnit(), GetItemTypeId(GetManipulatedItem())))
            return
        endif //Попытались подобрать чужой предмет
        if ( GetItemPlayer(GetManipulatedItem()) != Player(15) and GetItemPlayer(GetManipulatedItem()) != GetTriggerPlayer() and udg_Sandbox != 11 ) then
            call DisplayTimedTextToPlayer(GetOwningPlayer(GetTriggerUnit()), 0, 0, 15, "Ты не можешь поднять чужой предмет!")
            call UnitRemoveItem(GetTriggerUnit(), GetManipulatedItem())
            return
        endif //Мы подобрали предмет, а не купили руну
        if ( LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 7) == 0 ) then
            call SetItemPlayer(GetManipulatedItem(), GetTriggerPlayer(), false)
            return
        endif //Проверка, купили ли мы урну для игрока альянса и наоборот
        if ( ItemID == Urn and not ( IsPlayerInForce(GetTriggerPlayer(), HordeForce) ) ) then
            call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "Этот предмет доступен только игрокам орды! Его аналог для альянса - талисман леса")
            return
        endif
        if ( ItemID == Talisman_of_the_Wild and not ( IsPlayerInForce(GetTriggerPlayer(), AllianceForce) ) ) then
            call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "Этот предмет доступен только игрокам альянса! Его аналог для орды - урна")
            return
        endif
        if ( ItemID == Book_of_Agillity or ItemID == Book_of_Strength or ItemID == Book_of_Intelligence ) then
            if ( not ( TestItemAq__IsHeroInventoryFullWithUniqueItems(GetTriggerUnit()) ) ) then
                call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "Необходимо, что бы у вас инвентарь был заполнен уникальными предметами!")
                call SetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD, GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD) + price)
                return
            endif //Если у нас все уникальные - добавляем триггерно характеристику
            if ( ItemID == Book_of_Agillity ) then
                call SetHeroAgi(GetTriggerUnit(), GetHeroAgi(GetTriggerUnit(), false) + 1, true)
            endif
            if ( ItemID == Book_of_Intelligence ) then
                call SetHeroInt(GetTriggerUnit(), GetHeroInt(GetTriggerUnit(), false) + 1, true)
            endif
            if ( ItemID == Book_of_Strength ) then
                call SetHeroStr(GetTriggerUnit(), GetHeroStr(GetTriggerUnit(), false) + 1, true)
            endif
        endif
        if ( ItemID == Journeys_End ) then
            if ( GetUnitTypeId(GetTriggerUnit()) == Aegwynn or GetUnitTypeId(GetTriggerUnit()) == Archimonde or GetUnitTypeId(GetTriggerUnit()) == Arugal or GetUnitTypeId(GetTriggerUnit()) == Blackhand or GetUnitTypeId(GetTriggerUnit()) == ChoGall or GetUnitTypeId(GetTriggerUnit()) == Fenris or GetUnitTypeId(GetTriggerUnit()) == Galen or GetUnitTypeId(GetTriggerUnit()) == Garona or GetUnitTypeId(GetTriggerUnit()) == Grom or GetUnitTypeId(GetTriggerUnit()) == Curdran or GetUnitTypeId(GetTriggerUnit()) == Maim or GetUnitTypeId(GetTriggerUnit()) == Thoras or GetUnitTypeId(GetTriggerUnit()) == Darius or GetUnitTypeId(GetTriggerUnit()) == Tyralion ) then
                call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "Этот предмет недоступен для вашего героя!")
                return
            endif
        endif //Мы купили предмет, который из ничего не собирается - значит он не должен быть руной и просто ничего не делаем
        if ( LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 0) == 0 ) then
            return
        endif
        set i=0 //Смотрим для всех 6 мест под собирающийся айтем, есть ли он
        loop
        exitwhen ( i > 5 )
            set TargetItemID=LoadInteger(TestItemAq__ItemDataHashtable, ItemID, i) //Если доп айтем под слотом i не существует - мы перебрали все доп айтемы
            exitwhen ( TargetItemID == 0 ) //Высчитываем то, сколько надо потратить денег на то, что бы получить под-айтем (если он уже есть - значение будет равно 0)
            set price=price + TestItemAq__CountOfItemPointsForItem(TargetItemID , GetTriggerUnit())
        set i=i + 1
        endloop
        if ( GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD) < price ) then
            call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "Вам необходимо ещё: " + I2S(price - GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD)) + " очков предметов.")
            set i=0
            loop
            exitwhen ( i > 5 )
                if ( s__TestItemAq__RemovedItem[i] > 0 ) then
                    call UnitAddItemToSlotById(GetTriggerUnit(), s__TestItemAq__RemovedItem[i], s__TestItemAq__RemovedItemPosition[i])
                endif
                set s__TestItemAq__RemovedItem[i]= 0
                set s__TestItemAq__RemovedItemPosition[i]= - 1
            set i=i + 1
            endloop
            set TestItemAq__RemovedItemIndex=0
            return
        endif
        if ( TestItemAq__GetHeroFreeSlots(GetTriggerUnit()) + TestItemAq__RemovedItemIndex == 0 ) then
            call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "У вас заполнен инвентарь!")
            set i=0
            loop
            exitwhen ( i > 5 )
                if ( s__TestItemAq__RemovedItem[i] > 0 ) then
                    call UnitAddItemToSlotById(GetTriggerUnit(), s__TestItemAq__RemovedItem[i], s__TestItemAq__RemovedItemPosition[i])
                endif
                set s__TestItemAq__RemovedItem[i]= 0
                set s__TestItemAq__RemovedItemPosition[i]= - 1
            set i=i + 1
            endloop
            set TestItemAq__RemovedItemIndex=0
            return
        endif
        call SetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD, GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD) - price)
        set e=AddSpecialEffectTarget("Abilities\\Spells\\Items\\AIlm\\AIlmTarget.mdl", GetTriggerUnit(), "origin")
        if ( TestItemAq__ItemHasTwoVariants(ItemID) ) then
            if ( IsHeroRange(GetTriggerUnit()) ) then
                call SetItemPlayer(UnitAddItemById(GetTriggerUnit(), LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 7)), GetTriggerPlayer(), false)
            else
                call SetItemPlayer(UnitAddItemById(GetTriggerUnit(), LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 8)), GetTriggerPlayer(), false)
            endif
        else
            call SetItemPlayer(UnitAddItemById(GetTriggerUnit(), LoadInteger(TestItemAq__ItemDataHashtable, ItemID, 7)), GetTriggerPlayer(), false)
        endif
        set i=0
        loop
        exitwhen ( i > 5 )
            set s__TestItemAq__RemovedItem[i]= 0
            set s__TestItemAq__RemovedItemPosition[i]= - 1
        set i=i + 1
        endloop
        set TestItemAq__RemovedItemIndex=0
    endfunction
