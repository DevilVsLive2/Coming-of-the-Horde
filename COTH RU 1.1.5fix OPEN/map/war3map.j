    function TeronAura__AuraConditions takes nothing returns boolean
        return ( IsUnitEnemy(GetFilterUnit(), GetOwningPlayer(TeronUnit)) and GetUnitTypeId(GetFilterUnit()) != 'o008' and not ( IsUnitType(GetFilterUnit(), UNIT_TYPE_MECHANICAL) ) and not ( UnitHasBuffBJ(GetFilterUnit(), 'BHds') ) and not ( UnitHasBuffBJ(GetFilterUnit(), 'B03N') ) and not ( UnitHasBuffBJ(GetFilterUnit(), 'Bams') ) and not ( UnitHasBuffBJ(GetFilterUnit(), 'Bam2') ) and IsUnitAliveBJ(TeronUnit) )
    endfunction  //Выбранный юнит не пустышка
    function TeronAura__AuraDamage takes nothing returns nothing
        call UnitDamageTarget(TeronUnit, GetEnumUnit(), GetUnitAbilityLevel(TeronUnit, 'A02N') * 0.5, true, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
    endfunction
    function TeronAura__TeronEnumUnits takes nothing returns nothing
        local location TeronLoc=GetUnitLoc(TeronUnit)
        local group g=CreateGroup()
        call GroupEnumUnitsInRangeOfLoc(g, TeronLoc, 685, Condition(function TeronAura__AuraConditions))
        call ForGroup(g, function TeronAura__AuraDamage)
        call DestroyGroup(g)
        set g=null
        call RemoveLocation(TeronLoc)
        set TeronLoc=null
    endfunction
    function TeronAuraTrigger takes nothing returns nothing
