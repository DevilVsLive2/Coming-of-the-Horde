    function Sandbox__AddSpeed takes nothing returns nothing
        local integer CountOfMS
        local unit Target=LoadUnitHandle(Sandbox__SandboxHash, GetHandleId(GetTriggeringTrigger()), 0)
        set CountOfMS=( S2I(GetEventPlayerChatString()) )
        if ( GetUnitMoveSpeed(Target) + S2I(GetEventPlayerChatString()) > 522 ) then
            set CountOfMS=R2I(522 - GetUnitMoveSpeed(Target))
        endif
        call SetUnitMoveSpeed(Target, GetUnitMoveSpeed(Target) + CountOfMS)
        call DisplayTextToPlayer(GetTriggerPlayer(), 0, 0, "К скорости героя " + GetUnitName(LoadUnitHandle(Sandbox__SandboxHash, GetHandleId(GetTriggeringTrigger()), 0)) + " успешно добавлено " + I2S(CountOfMS) + " скорости. Текущая скорость: " + I2S(R2I(GetUnitMoveSpeed(Target))))
