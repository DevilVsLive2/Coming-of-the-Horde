                            set slowDummyCaster=CreateUnit(GetOwningPlayer(caster), 'o008', GetUnitX(target), GetUnitY(target), 0)
                            call UnitApplyTimedLife(slowDummyCaster, 'BTLF', 3)
                            call UnitAddAbility(slowDummyCaster, 'SHSL')
                            call SetUnitAbilityLevel(slowDummyCaster, 'SHSL', GetUnitAbilityLevel(caster, stormHammer))
                            call IssueTargetOrder(slowDummyCaster, "slow", target)
