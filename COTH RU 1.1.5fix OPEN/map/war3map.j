        set ArmorCount=ArmorCount + CountOfItems(whichHero , Boots_of_Speed) * 1
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Belt_of_Giants) * 1
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Veterans_Blade_Inventory) * 2
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Runed_Bracers_Inventory) * 4
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Helm_Of_Energy_Inventory) * 6
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Plated_Boots_Inventory) * 2
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Ring_of_Titus_Inventory) * 3
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Battlehirst_Helm_Inventory) * 6
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Bloodmoon_Plate_Inventory) * 6
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Sunscale_Helm_Inventory) * 8
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Bronze_Greaves_Inventory) * 5
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Dragonscale_Sheath_Inventory) * 14
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Bastion_of_Purity_Inventory) * 6
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Ring_of_Protection_RDD_Inventory) * 4
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Stonepath_Chestguard_Inventory) * 10
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Aegis_Gloves_Inventory) * 2
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Starlights_Inventory) * 6
        set ArmorCount=ArmorCount + CountOfItems(whichHero , Chest_Plate) * 8
        set ArmorCount=ArmorCount + ( HeroAgi * AGI_DEFENSE_BONUS )
        set ArmorCount=ArmorCount + ( GetUnitDefaultArmor(whichHero) - 2 )
        if ( ModuloInteger(HeroDefaultAgi, 5) >= 3 ) then
            set ArmorCount=ArmorCount + I2R(( HeroDefaultAgi / 5 ) + 1)
        else
            set ArmorCount=ArmorCount + I2R(HeroDefaultAgi / 5)
        endif
        if ( UnitHasBuffBJ(whichHero, 'BNsa') ) then
            set ArmorCount=ArmorCount + 2
        endif
        if ( UnitHasBuffBJ(whichHero, 'Bfae') ) then
            set ArmorCount=ArmorCount - 3
        endif
        if ( UnitHasBuffBJ(whichHero, 'Brpb') ) then
            set ArmorCount=ArmorCount + 4
        endif
        if ( UnitHasBuffBJ(whichHero, 'BEst') ) then
            set ArmorCount=ArmorCount + 8
        endif
        return ArmorCount
    endfunction
