    function MaxUpgrades__Upgrade takes nothing returns nothing
        local integer Research=GetResearched()
        local player TriggerPlayer=GetTriggerPlayer()
        if ( Research == Max_Armor or Research == Max_Armor_Gnome ) then //Макс апы
            call SetPlayerTechResearched(TriggerPlayer, Armor, 3)
            call SetPlayerTechResearched(TriggerPlayer, Armor_Gnome, 3)
            return
        endif
        if ( Research == Max_Damage or Research == Max_Damage_Gnome ) then
            call SetPlayerTechResearched(TriggerPlayer, Damage, 3)
            call SetPlayerTechResearched(TriggerPlayer, Damage_Gnome, 3)
            return
        endif
        if ( Research == Max_RangeDamage ) then
            call SetPlayerTechResearched(TriggerPlayer, RangeDamage, 3)
            return
        endif //Если начали делать слабые - убираем возможность замаксить одним апом
        if ( Research == Armor or Research == Armor_Gnome ) then
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Armor, 0)
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Armor_Gnome, 0)
            return
        endif
        if ( Research == Damage or Research == Damage_Gnome ) then
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Damage, 0)
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Damage_Gnome, 0)
            return
        endif
        if ( Research == RangeDamage ) then
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_RangeDamage, 0)
            return
        endif //Орда
        if ( Research == Max_Armor_Horde ) then //Макс апы
            call SetPlayerTechResearched(TriggerPlayer, Armor_Horde, 3)
            return
        endif
        if ( Research == Max_Damage_Horde ) then
            call SetPlayerTechResearched(TriggerPlayer, Damage_Horde, 3)
            return
        endif //Если начали делать слабые - убираем возможность замаксить одним апом
        if ( Research == Armor_Horde ) then
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Armor_Horde, 0)
            return
        endif
        if ( Research == Damage_Horde ) then
            call SetPlayerTechMaxAllowed(TriggerPlayer, Max_Damage_Horde, 0)
            return
        endif
        set TriggerPlayer=null
    endfunction
    function MaxUpgrades__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        local integer index
        set index=0
        loop
        exitwhen ( index > 11 )
            call TriggerRegisterPlayerUnitEvent(t, Player(index), EVENT_PLAYER_UNIT_RESEARCH_FINISH, null)
        set index=index + 1
        endloop
        call TriggerAddAction(t, function MaxUpgrades__Upgrade)
    endfunction

