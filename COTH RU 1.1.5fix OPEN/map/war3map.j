    function GlacialShield__IsAbillityAGlacial takes nothing returns boolean
        return ( GetSpellAbilityId() == 'AIhb' )
    endfunction
    function GlacialShield__RemoveAncient takes nothing returns nothing
        call UnitRemoveType(LoadUnitHandle(GlacialShield__GlacialHashtable, 0, GetHandleId(GetExpiredTimer())), UNIT_TYPE_ANCIENT)
        call PauseTimer(GetExpiredTimer())
        call DestroyTimer(GetExpiredTimer())
    endfunction
    function GlacialShield__GlacialUsed takes nothing returns nothing
        local unit GlacialTarget=GetSpellTargetUnit()
        local unit u=CreateUnit(GetOwningPlayer(GlacialTarget), 'o008', GetUnitX(GlacialTarget), GetUnitY(GlacialTarget), 0)
        call UnitAddType(GetSpellAbilityUnit(), UNIT_TYPE_ANCIENT)
        call UnitAddAbility(u, 'Aams')
        call IssueTargetOrder(u, "antimagicshell", GlacialTarget)
        call UnitApplyTimedLife(u, 'BTLF', 1)
        call PauseTimer(s__GlacialShield__GlacialTimer[GetPlayerId(GetOwningPlayer(GlacialTarget))])
        call DestroyTimer(s__GlacialShield__GlacialTimer[GetPlayerId(GetOwningPlayer(GlacialTarget))])
        set s__GlacialShield__GlacialTimer[GetPlayerId(GetOwningPlayer(GlacialTarget))]= CreateTimer()
        call TimerStart(s__GlacialShield__GlacialTimer[GetPlayerId(GetOwningPlayer(GlacialTarget))], 8, false, function GlacialShield__RemoveAncient)
        call SaveUnitHandle(GlacialShield__GlacialHashtable, 0, GetHandleId(s__GlacialShield__GlacialTimer[GetPlayerId(GetOwningPlayer(GlacialTarget))]), GlacialTarget)
        set GlacialTarget=null
        set u=null
    endfunction
    function GlacialShield__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        call TriggerRegisterAnyUnitEventBJ(t, EVENT_PLAYER_UNIT_SPELL_EFFECT)
        call TriggerAddCondition(t, Condition(function GlacialShield__IsAbillityAGlacial))
        call TriggerAddAction(t, function GlacialShield__GlacialUsed)
    endfunction

