    function MassThunderbolt__Thunderbolts_Cond takes nothing returns boolean
        return ( IsUnitEnemy(GetFilterUnit(), GetOwningPlayer(MassThunderbolt__TempUnit)) and not ( IsUnitType(GetFilterUnit(), UNIT_TYPE_STRUCTURE) ) )
    endfunction
    function MassThunderbolt__Thunderbolt takes nothing returns nothing
        local unit u=CreateUnitAtLoc(GetOwningPlayer(MassThunderbolt__TempUnit), 'o008', GetUnitLoc(GetEnumUnit()), 0)
        call UnitAddAbility(u, 'A0GL')
        call SetUnitAbilityLevel(u, 'A0GL', GetUnitAbilityLevel(MassThunderbolt__TempUnit, 'A03Q'))
        call IssueTargetOrder(u, "thunderbolt", GetEnumUnit())
        call UnitApplyTimedLife(u, 'BTLF', 1)
        set u=null
    endfunction
    function MassThunderbolt__MovingUnit takes nothing returns nothing
        local unit u
        local group g=CreateGroup()
        if ( GetSpellAbilityId() != 'A03Q' ) then
            return
        endif
        set MassThunderbolt__TempUnit=GetTriggerUnit()
        set MassThunderbolt__MoveLoc=GetSpellTargetLoc()
        set u=CreateUnit(GetOwningPlayer(GetTriggerUnit()), 'h01G', GetUnitX(GetTriggerUnit()), GetUnitY(GetTriggerUnit()), AngleBetweenPoints(GetUnitLoc(GetTriggerUnit()), MassThunderbolt__MoveLoc))
        call IssuePointOrder(u, "move", GetLocationX(MassThunderbolt__MoveLoc), GetLocationY(MassThunderbolt__MoveLoc))
        call PolledWait(DistanceBetweenPoints(GetUnitLoc(u), MassThunderbolt__MoveLoc) / 522 - 0.2)
        call GroupEnumUnitsInRangeOfLoc(g, MassThunderbolt__MoveLoc, 175, Condition(function MassThunderbolt__Thunderbolts_Cond))
        call ForGroup(g, function MassThunderbolt__Thunderbolt)
        call KillUnit(u)
        call TriggerSleepAction(1)
        call RemoveUnit(u)
        set u=null
        call DestroyGroup(g)
        set g=null
    endfunction
    function MassThunderbolt__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        call TriggerSleepAction(1)
        call TriggerRegisterUnitEvent(t, gg_unit_Hmkg_0476, EVENT_UNIT_SPELL_EFFECT)
        call TriggerAddAction(t, function MassThunderbolt__MovingUnit)
    endfunction

