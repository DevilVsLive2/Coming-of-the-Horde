    function GennVengeance__Vengeance_Conditions takes nothing returns boolean
        return ( not ( IsUnitType(GetDyingUnit(), UNIT_TYPE_STRUCTURE) ) and IsUnitEnemy(GetKillingUnit(), GetOwningPlayer(GetDyingUnit())) )
    endfunction
    function GennVengeance__IsLearnedSpellAVengeance takes nothing returns boolean
        return ( GetLearnedSkill() == 'A03W' )
    endfunction
    function GennVengeance__Vengeance_Downgrade takes nothing returns nothing
        if ( ( GetUnitTypeId(GetRevivingUnit()) != 'Hpb2' ) ) then
            return
        endif
        if ( GetUnitAbilityLevel(GetRevivingUnit(), 'A0GJ') >= 1 ) then
            call SetUnitAbilityLevel(GetRevivingUnit(), 'A0GJ', ( GetUnitAbilityLevel(GetRevivingUnit(), 'A0GJ') / 2 ))
            call SetUnitAbilityLevel(GetRevivingUnit(), 'A0GI', ( GetUnitAbilityLevel(GetRevivingUnit(), 'A0GI') / 2 ))
            call SetUnitAbilityLevel(GetRevivingUnit(), 'A03U', ( GetUnitAbilityLevel(GetRevivingUnit(), 'A03U') / 2 ))
            call SetUnitAbilityLevel(GetRevivingUnit(), 'A03T', ( GetUnitAbilityLevel(GetRevivingUnit(), 'A03T') / 2 ))
            call SetUnitAbilityLevel(GetRevivingUnit(), 'A03V', ( GetUnitAbilityLevel(GetRevivingUnit(), 'A03V') / 2 ))
        endif
    endfunction
    function GennVengeance__Vengeance_Upgrade takes nothing returns nothing
        if ( IsUnitDeadBJ(udg_Greymane) ) then
            return
        endif
        if ( GetUnitAbilityLevel(udg_Greymane, 'A03W') < 1 ) then
            return
        endif
        if ( GetUnitAbilityLevel(udg_Greymane, 'A0GJ') <= ( GetUnitAbilityLevel(udg_Greymane, 'A03W') * 5 ) ) then
            call IncUnitAbilityLevel(udg_Greymane, 'A0GJ')
            call IncUnitAbilityLevel(udg_Greymane, 'A0GI')
        endif
        if ( GetUnitAbilityLevel(udg_Greymane, 'A03W') >= 1 and GetUnitAbilityLevel(udg_Greymane, 'A03U') < 11 ) then
            call IncUnitAbilityLevel(udg_Greymane, 'A03U')
        endif
        if ( GetUnitAbilityLevel(udg_Greymane, 'A03W') >= 2 and GetUnitAbilityLevel(udg_Greymane, 'A03T') < 11 ) then
            call IncUnitAbilityLevel(udg_Greymane, 'A03T')
        endif
        if ( GetUnitAbilityLevel(udg_Greymane, 'A03W') >= 3 and GetUnitAbilityLevel(udg_Greymane, 'A03V') < 11 ) then
            call IncUnitAbilityLevel(udg_Greymane, 'A03V')
        endif
    endfunction
    function GennVengeance__Add_Vengeance takes nothing returns nothing
        if ( GetLearnedSkillLevel() == 1 ) then
            call UnitAddAbility(GetTriggerUnit(), 'A03U')
        elseif ( GetLearnedSkillLevel() == 2 ) then
            call UnitAddAbility(GetTriggerUnit(), 'A03T')
        elseif ( GetLearnedSkillLevel() == 3 ) then
            call UnitAddAbility(GetTriggerUnit(), 'A03V')
        endif
    endfunction
    function GennVengeance__onInit takes nothing returns nothing
        local trigger t=CreateTrigger()
        call TriggerRegisterPlayerUnitEvent(t, Player(7), EVENT_PLAYER_HERO_SKILL, Condition(function GennVengeance__IsLearnedSpellAVengeance))
        call TriggerAddAction(t, function GennVengeance__Add_Vengeance)
        set t=CreateTrigger()
        call TriggerRegisterPlayerUnitEvent(t, Player(7), EVENT_PLAYER_UNIT_DEATH, Condition(function GennVengeance__Vengeance_Conditions))
        call TriggerAddAction(t, function GennVengeance__Vengeance_Upgrade)
        call TriggerSleepAction(1)
        set t=CreateTrigger()
        call TriggerRegisterUnitEvent(t, gg_unit_n00S_0289, EVENT_UNIT_HERO_REVIVE_FINISH)
        call TriggerRegisterUnitEvent(t, gg_unit_n00P_0298, EVENT_UNIT_HERO_REVIVE_FINISH)
        call TriggerAddAction(t, function GennVengeance__Vengeance_Downgrade)
    endfunction

