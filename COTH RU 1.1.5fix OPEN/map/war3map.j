    function UnitDamaged__UnitOnDamage takes nothing returns nothing
        local unit Damager=GetAttacker()
        local unit Target=GetTriggerUnit()
        local timer BashCooldownTimer
        local unit Placeholder
        local boolean IsApplyStun=false
        local real l__Damage=GetEventDamage()
        local boolean IsTargetAllyDamager=IsUnitAlly(Target, GetOwningPlayer(Damager))
        local boolean WisdomDamageTarget=false
        local real Mana
        local real AmaniTimerSeconds=3
        local real BlackrockTimerSeconds=3
        local real CorruptionEdgeTimerSeconds=3
        local timer TempTimer
        local timer AmaniThrowAxeTimer
        local timer BlackrockChampionsAxeTimer
        local timer CorruptionEdgeTimer
        local trigger TriggeringTrg=GetTriggeringTrigger()
        local integer UnitArmor
        local real UnitFloatArmor
        local unit TempUnit
        local location TempLoc
        local location TempLocSecond
        local texttag GaronaDamage
        if ( IsUnitInGroup(Damager, udg_StopGroup) ) then //Стан
            return
        endif
        if ( udg_Sandbox == 11 ) then
            set IsTargetAllyDamager=false
        endif
        if ( ( UnitHasItemOfTypeBJ(Damager, Royal_Hammer_Inventory_Melee) or UnitHasItemOfTypeBJ(Damager, Royal_Hammer_Inventory_Range) or ( GetUnitAbilityLevel(Damager, ABILITY_STUN) > 0 ) ) and not IsTargetAllyDamager ) then
            if ( UnitHasItemOfTypeBJ(Damager, Royal_Hammer_Inventory_Melee) ) then
                if ( GetRandomInt(1, 100) <= 16 ) then
                    set IsApplyStun=true
                endif
            endif
            if ( UnitHasItemOfTypeBJ(Damager, Royal_Hammer_Inventory_Range) ) then
                if ( GetRandomInt(1, 100) <= 8 ) then
                    set IsApplyStun=true
                endif
            endif
            if ( GetUnitAbilityLevel(Damager, 'ACbh') > 0 ) then
                if ( GetRandomInt(1, 100) <= 10 ) then
                    set IsApplyStun=true
                endif
            endif
            if ( IsHeroUnitId(GetUnitTypeId(Target)) ) then
                if ( LoadBoolean(UnitDamaged__UnitDataHashtable, GetHandleId(Damager), UnitDamaged__HASHTABLE_IS_UNIT_STAN_ON_COOLDOWN) == false and IsApplyStun ) then
                    call SaveBoolean(UnitDamaged__UnitDataHashtable, GetHandleId(Damager), UnitDamaged__HASHTABLE_IS_UNIT_STAN_ON_COOLDOWN, true)
                    set BashCooldownTimer=CreateTimer()
                    call TimerStart(BashCooldownTimer, 3.5, false, function UnitDamaged__RemoveCooldownBash)
                    call SaveUnitHandle(UnitDamaged__UnitDataHashtable, GetHandleId(BashCooldownTimer), 0, Damager)
                    call UnitRemoveAbility(Target, GLACIAL_BUFF)
                    set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 0)
                    call UnitAddAbility(Placeholder, 'ANfb')
                    call DisableTrigger(TriggeringTrg)
                    call IssueTargetOrder(Placeholder, "firebolt", Target)
                    call EnableTrigger(TriggeringTrg)
                    call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                endif
            elseif ( IsApplyStun ) then
                set Placeholder=CreateUnit(GetOwningPlayer(Damager), 'o008', GetUnitX(Target), GetUnitY(Target), 0)
                call UnitAddAbility(Placeholder, 'ANfb')
                call DisableTrigger(TriggeringTrg)
                call IssueTargetOrder(Placeholder, "firebolt", Target)
                call UnitApplyTimedLife(Placeholder, 'BTLF', 1)
                call EnableTrigger(TriggeringTrg)
            endif
        endif
        if ( UnitHasItemOfTypeBJ(Damager, Wisdom_Carver_Inventory) and not IsTargetAllyDamager ) then //Разрушитель разума
            if ( IsHeroRange(Damager) ) then
                set Mana=GetHeroInt(Target, true) * 0.25
            else
                set Mana=GetHeroInt(Target, true) * 0.5
            endif
            call UnitRemoveMana(Target , Mana)
            call UnitAddMana(Damager , Mana)
            if ( GetRandomInt(1, 100) <= 40 ) then
                set WisdomDamageTarget=true
            endif
            if ( WisdomDamageTarget ) then
                call DisableTrigger(TriggeringTrg)
                call UnitDamageTarget(Damager, Target, Mana, true, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC, WEAPON_TYPE_WHOKNOWS)
