    function TestItemAq__ItemIndex takes integer itemType,unit whichUnit returns integer
        local integer i
        set i=0
        loop
        exitwhen ( i > 5 )
            if ( GetItemTypeId(UnitItemInSlot(whichUnit, i)) == itemType and UnitItemInSlot(whichUnit, i) != null ) then
                return i
            endif
        set i=i + 1
        endloop
        return - 1
